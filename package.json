{
  "name": "conf-cli",
  "description": "simple cli for configuration and key/value's",
  "version": "0.1.8",
  "author": "Nathaniel Camomot",
  "bin": {
    "conf": "./bin/run"
  },
  "bugs": "https://github.com/azedo/conf-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.11",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.1.4",
    "conf": "^2.2.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.5",
    "@oclif/test": "^1.2.2",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.2",
    "chai": "^4.2.0",
    "globby": "^9.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/azedo/conf-cli",
  "keywords": [
    "oclif",
    "conf",
    "cli",
    "config"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "conf",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "azedo/conf-cli",
  "scripts": {
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "tslint -p test -t stylish",
    "prepack": "oclif-dev manifest && rimraf lib && tsc -b",
    "postpack": "rimraf oclif.manifest.json"
  }
}
